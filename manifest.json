{
    "name": "LeParisien Premium",
    "version": "1.0",
    "description": "LeParisien Premium FREE",
    "manifest_version": 2,
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts":[
        {
            "matches": ["http://wwww.leparisien.fr/*", "https://www.leparisien.fr/*"],
            "js": ["jquery.js","index.js"]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions":[
        "activeTab",
        "storage",
        "activeTab",
        "tabs",
        "contextMenus",
        "https://.leparisien.fr/*"
    ]
}
